{
  "name": "@wrtnlabs/calculator-mcp",
  "type": "module",
  "version": "0.1.3",
  "packageManager": "pnpm@10.6.4",
  "description": "Calculator MCP",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrtnlabs/calculator-mcp"
  },
  "bugs": {
    "url": "https://github.com/wrtnlabs/calculator-mcp/issues"
  },
  "bin": "bin/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "build": "unbuild",
    "publint": "publint",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm prepack && publint && pnpm dlx bumpp"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@types/node": "^20.14.0",
    "@wrtnlabs/eslint-config": "^0.4.3",
    "commander": "^13.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "publint": "^0.3.10",
    "std-env": "^3.9.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  }
}
